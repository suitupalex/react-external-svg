{
  "name": "react-external-svg",
  "version": "0.1.0",
  "description": "A React component that simplifies inline loading of an external SVG.",
  "main": "react-external-svg.jsx",
  "scripts": {
    "lint": "./scripts/lint",
    "test": "./scripts/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suitupalex/react-external-svg.git"
  },
  "keywords": [
    "react",
    "component",
    "svg",
    "external"
  ],
  "author": "Alexander Martin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suitupalex/react-external-svg/issues"
  },
  "homepage": "https://github.com/suitupalex/react-external-svg#readme",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-jest": "^11.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "debug": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "jest-cli": "^11.0.2",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-tools": "^0.13.3"
  },
  "jest": {
    "testDirectoryName": "tests",
    "testFileExtensions": [
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/react-external-svg.jsx",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-tools",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/superagent"
    ]
  },
  "dependencies": {
    "react": "^15.0.1",
    "superagent": "^1.8.3"
  }
}
